{
  "extends": [
    "next/core-web-vitals",
    "plugin:storybook/recommended",
    "eslint-config-prettier"
  ],
  "rules": {
    "import/no-anonymous-default-export": "off",
    "no-console": "error",
    "eqeqeq": "error",
    "no-unused-vars": [
      "error",
      {
        "args": "all",
        "caughtErrors": "all",
        "argsIgnorePattern": "^_",
        "caughtErrorsIgnorePattern": "^_",
        "destructuredArrayIgnorePattern": "^_"
      }
    ]
  }
}
