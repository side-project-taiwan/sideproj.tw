{
  "author": "Kirby Wu",
  "name": "grants-landing-template",
  "description": "grants default landing page template",
  "version": "0.0.1",
  "homepage": "https://github.com/g0v/grants-landing-template",
  "repository": {
    "type": "git",
    "url": "https://github.com/g0v/grants-landing-template"
  },
  "engines": {
    "node": ">=0.12.6",
    "npm": ">=1.2.2"
  },
  "scripts": {
    "prebuild": "rm -rf build && mkdir build && cp -r src/img build && cp src/index.css build",
    "build": "pug src/index.jade --out ./build --pretty",
    
    "start": "node_modules/.bin/lsc server.ls",
    "restart": "node_modules\\.bin\\lsc server.ls",
    "prepare": "npm install chokidar@1.7.0"
  },
  "dependencies": {
    "chokidar": "^1.7.0",
    "jade": "^1.11.0",
    "js-yaml": "^3.10.0",
    "LiveScript": ">=1.2.0",
    "markdown": "=0.5.0",
    "pug-cli": "^1.0.0-alpha6",
    "stylus": "=0.50.0",
    "uglify-js": "=3.2.0"
  }
}
